#!/bin/sh

if [ ! "$QBHOME" ]; then
    PRG=$0
    
    progdir=`dirname "$PRG"`
    
    if [ "`echo "$progdir" | grep -s '^/'`" ]; then
        progdir=$progdir
    else
        progdir=`pwd`/"$progdir"
    fi
    
    QBHOME=`echo "$progdir" | sed 's:/bin$::'`
    export QBHOME
fi

if [ -z "$QBENVSET" ]; then
    . $QBHOME/etc/qbenv.sh
fi

export R_HOME=$QBHOME/lib/R

pwd
which rscriptor
echo "QBHOME=" $QBHOME
echo "LD_LIBRARY_PATH=" $LD_LIBRARY_PATH
rscriptor 9090
ldd ../../DivConDiscoverySuit/bin/rscriptor
#moedivconanalysis.bin $@ - 
