#!/bin/sh

if [ ! "$QBHOME" ]; then
    PRG=$0
    
    progdir=`dirname "$PRG"`
    if [ "$progdir" = "." ]; then
	    progdir=`pwd`
    fi
    
    if [ "`echo "$progdir" | grep -s '^/'`" ]; then
        progdir=$progdir
    else
        progdir=`pwd`/"$progdir"
    fi
    
    QBHOME=`echo "$progdir" | sed 's:/bin$::'`
    export QBHOME
fi

. $QBHOME/etc/qbenv.sh

if [ -e $QBHOME/${PLTFM_VER}/bin/moedivconanalysis.bin ]; then
    exec "$QBHOME/${PLTFM_VER}/bin/moedivconanalysis.bin" "$@" "-" 
fi

exit $?
 
